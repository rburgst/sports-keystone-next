    "test": "jest --timeout 60",
    "seed-data": "keystone-next --seed-data"
  },
  "dependencies": {
    "@keystone-next/auth": "^34.0.0",
    "@keystone-next/fields-document": "^11.0.0",
    "@keystone-next/keystone": "^26.1.1",
    "@prisma/client": "latest",
    "next": "^11.1.2",
    "next-auth": "^3.29.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.3.4",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.33",
    "autoprefixer": "^10.4.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.1.0",
    "eslint-config-next": "^12.0.1",
    "jest": "^27.3.1",
    "postcss": "^8.3.11",
    "prettier": "^2.4.1",
    "prisma": "latest",
    "tailwindcss": "^2.2.19",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  },
  "packageManager": "yarn@3.0.2"
